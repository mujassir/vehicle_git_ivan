@charset "utf-8";
@import "general.css";
@import "fonts.css";

	/* Header Start */
	.header{ position:relative;}
		.headerTop{ min-height:84px; background:#FFF;}

			/* Logo Begin */
			.logo{ position:absolute; left:18px; top:3px; }
				.logo h1{ font-size:1.6em; line-height:77px; margin:0px;}
					.logo h1 span{ display:none;}

			/* Top Links Begin */
			.topLinks{ position:absolute; top:28px; right:294px;}
				.topLinks ul li{
					background:url(../images/topIcon.gif) no-repeat 0 3px scroll;
					padding-left:33px;
					float:left;
					margin-right:20px;
					height:22px;
					line-height:23px;
					text-transform:uppercase;
					font-size:0.58em;
					font-weight:bold;
					}
					.topLinks ul li ~ li{ background:url(../images/topIcon.gif) no-repeat 0 -28px scroll; }
						.topLinks ul li a{ color:#000099;}
							.topLinks ul li a:hover{ color:#ff9900;}

			/* Top Links Begin */
			.topSerach{ position:absolute; top:7px; right:10px;}
				.topSerach input[type="text"]{ background:url(../images/searchfiled.png) no-repeat 0 0 scroll; width:160px; height:35px; line-height:35px; padding-left:10px;	}
				.topSerach input[type="text"], .topSerach input[type="image"]{ float:left;}

			/* Top Links Begin */
			.topContract{ position:absolute; top:10px; right:13px; height:55px; line-height:55px; border-left:1px solid #666; padding-left:75px;}
				.topContract img{ float:left; margin:10px 22px 0px 0px;}
				.topContract h5{ background:url(../images/contractArrow.gif) no-repeat 0 24px scroll; padding-left:24px; float:left; font-size:0.85em;}
					.topContract h5, .topContract h5 a{color:#999999;}
										.topContract h5 a:hover{color:#000066;}

			.PlogoWrap{  top:0px; margin:40px 0 0 50px; float:left; padding-left:0px;}
				.PlogoWrap ul{ list-style:none; margin-left:0px;}
					.PlogoWrap ul li{ float:left; margin:0px 0px 0px 0px; padding:0 7px; }

		.imgright{margin-left:8px; margin-bottom:6px; float:right;}
			.imgright ul{ list-style:none; margin-left:0px;}
			.imgright ul li{ background:url(../images/Icon.jpg) no-repeat; font:normal 20px Arial, Helvetica, sans-serif; color:#666666; margin:0px; padding:12px 0 24px 65px; }
					.imgright ul li p{ margin:0px; padding:0px; font:normal 11px Arial, Helvetica, sans-serif; color:#666666; }
						.imgright h2{ font:normal 18px Arial, Helvetica, sans-serif; color:#666666; margin:0px; padding:0px; text-transform:capitalize; }

		/* Banner Begin */
		.navArea{ z-index:999; top:15px; position:absolute;}
			.navigation{ background:url(../images/navLeft.png) no-repeat 0 0 scroll; padding-left:12px; height:48px; width:1018px}
				.navigation div{ background:url(../images/navRight.png) no-repeat right 0 scroll; padding-right:12px;}
					.navigation div div{ background:url(../images/navMid.png) repeat-x 0 0 scroll; padding:0px;}
						.navigation div div div{ background:none; padding:0px;}
							.navigation ul li{
								background:url(../images/navSeprator.png) no-repeat 0 0 scroll; float:left; text-transform:uppercase; font-weight:bold; font-size:0.85em;}
							.navigation ul li:first-child{ background:none;}
								.navigation ul li a{ padding:0px 20px 0px 21px; line-height:48px; display:block; color:#FFF;  }
								.navigation ul li a.home{ background:url(../images/home.png) no-repeat center center scroll; padding:0px 47px 0px 40px; height:45px;}
								.navigation ul li a.search{ background:url(../images/search.png) no-repeat right center scroll; padding:0px 23px; height:45px;  }
								.navigation ul li a.paddingRight{ padding-right:78px !important;}
									.navigation ul li a:hover, .navigation ul li a.active{ color:#ffca88;}

		/* Banner Begin */
		.banner{ height:354px; overflow:hidden; background:url(../images/bannerBg.gif) repeat-x 0 bottom scroll; padding-bottom:15px;}
			.banner .slide{ position:relative !important;}
			.banner .slideText{ position:absolute; right:-3px; top:92px; width:398px; z-index:999;}
				.banner .slideText h1, .banner .slideText h2{color:#ff9900; font-weight:normal;}
				.banner .slideText h1, .banner .slideText h2, .banner .slideText p{text-transform:uppercase; margin-bottom:18px; font-weight:normal;}
				.banner .slideText h1{font-size:1.875em;}
				.banner .slideText h2{font-size:1.5em;}
				.banner .slideText p{ font-size:1em; color:#333333;}
				.banner .slideText a.largBtn{ margin-top:5px;}
				.banner .mainBtn{ position:absolute; left:0px; top:306px; z-index:999;}
					.mainBtn a.roundBtn{ margin-right:10px;}
				.banner .slidePicture{ position:absolute; width:1160px; left:-64px;}

			.banInner{ position:relative; left:-60px;}

		/* Body Content Begin */
		.bodyContent{ background:url(../images/bodyContent.gif) repeat-x 0 bottom scroll #FFF;}
			.bodyContent h1{color:#ff9900; font-weight:normal;}
			.bodyContent  h3{color:#000066; font-size:1.37em;}
			.bodyContent  p{ text-align:left;}

		.mainContent{ padding:15px 0px; }
		.innerContent .wrap{ background:#FFF; padding:12px 0px 25px 0px;}
			.innerContent h1{ font-weight:bold; margin-bottom:8px; }
			.innerContent h6{color:#ff9900; font-size:0.98em; }

		.bottomContent{ border-top:1px solid #000066; padding:23px 0px;}
			.bottomContent a{ font-weight:bold; font-size:0.75em; padding-top:2px; display:block; color:#000066;}
				.bottomContent a:hover{ text-decoration:underline;}
			.bottomContent p{ margin:12px 0px 0px 0px}
			.bottomContent p:first-child{ margin:0px !important;}

				/* Main Conetnt Info Begin */
				.sideContact ul li{ border:1px solid #cccccc; margin-bottom:5px;}
					.sideContact ul li img{ float:left; margin-right:15px;}
					.sideContact ul li h6{ margin-bottom:4px; font-size:0.7em; color:#000066; text-transform:uppercase;}
						.sideContact ul li h6 a{ color:#000066;}
							.sideContact ul li h6 a:hover{ text-decoration:underline;}
					.sideContact ul li ul li{ border:none; color:#000000; text-transform:uppercase; margin:0px;}
					.sideContact ul li .padding{ padding:8px 8px 0px 8px !important; }
					.sideContact ul li ul li:before{ content:"»"; padding-right:5px; float:left;}
						.sideContact ul li ul li{font-size:0.87em;}
							.sideContact ul li ul li a{ color:#000000;}
								.sideContact ul li ul li a:hover{text-decoration:underline;}
						.sideContact ul li ul li ul{ margin-left:95px; background:url(../images/emailIcon.gif) no-repeat 0 0 scroll; padding-left:40px; margin-top:3px;}
							.sideContact ul li ul li ul li{ font-size:0.81em; margin:0px;  font-weight:bold}
								.sideContact ul li ul li ul li a{ color:#000066;}


				/* Main Conetnt Form Begin */
				.requestfrom{ position:relative;}
					.requestfrom input[type="text"]{
						 background:url(../images/mainField.png) no-repeat 0 0 scroll; width:220px; height:34px; line-height:34px; padding:0px 10px; margin-bottom:12px;
						 float:left; text-transform:uppercase; font-size:0.81em;}
					.requestfrom input[type="image"]{ float:right;}

		/* inner Conetnt Form Begin */
		.sideBar{ width:190px;}
			.pointer{ background:url(../images/sideBg.gif) no-repeat 0 0 scroll; height:32px; line-height:32px; padding-left:10px;}
				.pointer h5{  font-family:Arial, Helvetica, sans-serif; font-weight:bold; color:#FFF; font-size:0.81em;}

			.sideBar ul li{ font-size:0.81em; text-transform:uppercase; font-weight:bold; border-top:1px solid #cccccc; }
			.sideBar ul li:first-child{ border-top: none;}
                        .sideBar ul li a{color:#999999; display:table-cell; height:37px; line-height:normal; width:188px; padding:1px 0px 0px 10px; vertical-align:middle; }

					.sideBar ul li:hover a, .sideBar ul li.active a{ color:#FFF; background:url(../images/Arrow-hover.png) no-repeat 0 0 scroll;}

		.contentInner{ width:780px; margin-right:45px;}

		/*.contentInner p span{color:#000066;}*/
			.services h2{ background:#292663; height:33px; line-height:33px; padding-left:5px; color:#FFF; width:530px;}
			.services h6{ margin:0px 0px 5px 0px; font-size:0.98em;}
				.services h5{ color:#FF9900; margin-bottom:8px;}
				.services ul { margin-left:12px; margin-bottom:10px;}
				.services ul li{ list-style:disc; font-size:0.75em; margin-bottom:4px; font-family:Arial, Helvetica, sans-serif;}

			.staff ul{ margin-bottom:12px;}
				.staff ul li{ float:left; width:129px; height:129px; overflow:hidden; margin-left:7px;}
				.staff ul li:first-child{margin-left:0px;}
				.staff p strong{ color:#110b74;}

			.contact{ border-right:1px solid #cccccc; padding-top:22px;}
				.contact input[type="text"], .contact textarea{
				background:url(../images/inputBg.gif) repeat-x 0 0 scroll;
				border:1px solid #b4babf;
				height:28px;
				ine-height:28px;
				padding:0px 10px;
				margin:0px 13px 20px 0px;
				width:240px;
				color:#828282;
				}
				.contact textarea{ height: 192px; padding-top:10px; width:430px; margin-bottom:10px;}
				.contact input[type="image"]{ margin-right:38px; float:right;}
					.column-260 p a, .column-260 ul li a:hover{ color:#000099;}
						.column-260 p a:hover{ color:#fdac33;}
					.column-260 ul{ margin-top:30px;}
					.column-260 ul li{
						background:url(../images/contractArrow.gif) no-repeat 0 3px scroll; margin-bottom:10px; padding-left:20px; font-size:0.85em; font-weight:bold;}
						.column-260 ul li a{ color:#fdac33}

	/* Footer Begin */
	.footer{position:relative; }
		.footer .footerTop{ background: url(../images/footerBg.gif) repeat-x 0 0 scroll; min-height:149px; padding:26px 0px; margin-bottom:12px; }

			.footInfo h6{ font-weight:bold;}
			.footInfo h6{ color:#ff9933; text-transform:none; font-size:0.75em; margin-bottom:18px;}
			.footInfo p{margin-bottom:5px; font-family:EurostileRegular; text-align:left; font-weight:normal; font-size:0.78em; }
				.footInfo p, .footInfo p a{color:#FFF;}
								.footInfo p a:hover{color:#ff9933;}
			.footInfo.marginRight{ margin-right:32px !important; }

			.footLinks ul { width:175px; float:left; border-left:1px solid #cccccc; padding-left:10px; height:127px;}
				.footLinks ul li{ text-transform:uppercase; font-size:0.81em; margin-bottom:10px; font-weight:bold;}
					.footLinks ul.mainLink{width:90px !important; border:none; }
					.footLinks ul.mainLink li:before{content:"»"; padding-right:6px; font-size:1.1em; }
					.footLinks ul li, .footLinks ul li a{ color:#ff9900;}
						.footLinks ul li a:hover{ color:#FFF;}
							.footLinks ul li ul{ margin:5px 0px 0px 0px; border:none; padding:0px;  }
								.footLinks ul li ul li{ font-size:0.89em; text-transform:none; margin-bottom:5px; background:url(../images/linkArrow.png) no-repeat 0 4px scroll; padding:0px 0px 0px 9px; letter-spacing:0.2px; font-weight:normal;}
								/*.footLinks ul li ul li:before{ content:"»"; padding-right:6px; font-size:1.3em; padding-top:2px; }*/
									.footLinks ul li ul li, .footLinks ul li ul li a{ color:#FFF;}
																.footLinks ul li ul li a:hover{ text-decoration:underline;}
			.footBot p{ text-align:center; color:#333333; font-size:0.6em; font-family:Arial, Helvetica, sans-serif; font-weight:bold; }
